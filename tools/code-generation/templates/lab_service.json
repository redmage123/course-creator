{
    "name": "lab-service",
    "description": "Interactive Lab Environment Service",
    "port": 8004,
    "endpoints": [
        {
            "name": "lab",
            "routes": [
                {
                    "method": "POST",
                    "path": "/labs",
                    "function_name": "create_lab",
                    "description": "Create a new lab exercise",
                    "return_type": "Dict[str, Any]",
                    "body_param": {"name": "request", "type": "LabCreateRequest"}
                },
                {
                    "method": "GET",
                    "path": "/labs/{lab_id}",
                    "function_name": "get_lab",
                    "description": "Get lab by ID",
                    "return_type": "Dict[str, Any]",
                    "path_params": [{"name": "lab_id", "type": "str"}]
                }
            ]
        }
    ],
    "dependencies": [
        "docker>=6.1.0",
        "websockets>=11.0.0"
    ],
    "database_models": [
        {
            "name": "Lab",
            "description": "Lab exercise definition",
            "fields": [
                {"name": "title", "type": "str", "nullable": false},
                {"name": "description", "type": "str", "nullable": false},
                {"name": "difficulty_level", "type": "str", "default": "'beginner'"}
            ]
        }
    ],
    "async_tasks": [],
    "websocket_endpoints": [],
    "middleware": ["logging", "timing"]
}
