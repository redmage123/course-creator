    <!-- Floating AI Assistant Button -->
    <button id="aiAssistantButton" onclick="toggleDashboardAIChat()" class="ai-assistant-fab" onmouseover="this.style.transform='scale(1.1)'" onmouseout="this.style.transform='scale(1)'">
        üí¨
